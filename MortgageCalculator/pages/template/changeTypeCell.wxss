.containCellView {
    display: flex;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    padding-right: 30rpx;
    padding-left: 30rpx;
  }

.contentDes {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333;
    letter-spacing: 0;
    flex: 1;
    text-align: left;
  }
  
  .changeContainView {
    height: 88rpx;
    flex: 3;
    line-height: 100rpx;
    display: flex;
  }
  
  .leftPart {
    display: flex;
    line-height: 88rpx;
    height: 88rpx;
    width: 220rpx;
    text-align: left;
  }
  
  .rightPart {
    display: flex;
    line-height: 88rpx;
    height: 88rpx;
    text-align: left;
    width: 220rpx;
  }
  
  .contentType {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #999;
    letter-spacing: 0;
    text-align: left;
    width: 160rpx
  }
  
  .outsideCircle {
    width: 24rpx;
    height: 24rpx;
    border-radius: 50%;
    border: 2rpx solid #ee3939;
    text-align: center;
    align-content: center;
    background-color: white;
    margin:auto;
    margin-left: 0rpx;
  }
  
  .insideCircle {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background-color: #ee3939;
    margin-left: 2rpx;
    margin-top: 2rpx;
  
  }

  .unselectCircle {
    background-color: white;
  }