.topDetailView {
  display: flex;
}

.everyItem {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.topTitleFont {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999;
  letter-spacing: 0;
}

.topContentFont {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #333;
  letter-spacing: 0;
}

.seportView {
  width: 100%;
  height: 20rpx;
  background-color: #f3f4f3;
}

.titleWrap {
  display: flex;
  margin-left: 30rpx;
  margin-right: 30rpx; 
}

.titleWrapToTop {
  display: flex;
  position: fixed;
  top: 0rpx;
  margin-left: 0rpx;
  margin-right: 0rpx; 
  width: 100%;
  background-color: white;
}

.titleContentFont {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 0;
  flex: 1;
  text-align: left;
}

.itemsMoneyWrap {
  display: flex;
}

.yearTitle {
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 0;
  text-align: left;
}

.itemsMoneyItem {
  display: flex;
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999;
  letter-spacing: 0;
}

.bottomMoneyView {
  margin-left: 30rpx;
  margin-right: 30rpx; 
}